#include <iostream>
using namespace std;
int pere(int array[],int N) {
    int j;
    for (int i = 0; i < N; i++)
        array[i] = i + 1;
    for (int i = N; i >= 1; i--) {
        j = rand() % (i);
        int t = array[i - 1];
        array[i - 1] = array[j];
        array[j] = t;
    }
}
void print_array(int array[],int z){
    int i;
    for(i=0;i<z;i++){
        cout<<array[i]<<" ";
    }

}
int main() {
    int n;
    cin>>n;
    int a[n];
    pere(a,n);
    print_array(a,n);
}
